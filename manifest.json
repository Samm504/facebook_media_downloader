{
  "manifest_version": 2,
  "name": "Facebook Media Downloader",
  "version": "1.0",
  "description": "Lists out all media files loaded on a webpage.",
  "permissions": ["storage", "downloads", "webRequest", "webRequestBlocking", "http://*/*", "https://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
